      "type": "string"
    },
    "timestamp": {
      "type": "string"
    },
    "parents": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "content": {
      "type": "object"
    },
    "provenance": {
      "type": "object"
    },
